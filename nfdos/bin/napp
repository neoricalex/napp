#!/home/neo/www/napp/nfdos/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ncli','console_scripts','napp'
__requires__ = 'ncli'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ncli', 'console_scripts', 'napp')()
    )
